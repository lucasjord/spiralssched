note2    = ''

! ==========================================================
! ==============  Correlator Information  ==================
! ==========================================================

correl   = LBA
coravg   = 2
corchan  = 4096
cornant  = 4
corpol   = 'off'
corwtfn  = 'uniform'
corsrcs  = 'from schedule'
cortape  = 'DAT'
corship1 = 'University of Tasmania'
cornote1 = 'Pass 1: Full band: 16 MHz. corchan=32, 0.5 MHz res'

! ==========================================================
! ==================  Program Control  =====================
! ==========================================================

overwrit
sumitem= ela, slew, early, dwell
!sumitem= slew, aza

! ==========================================================
! =================== Station Catalogs  ====================
! ==========================================================

freqfile = ./freq.spirals.dat
stafile  = ./stations.spirals.dat
locfile  = ./locations.spirals.dat

! ==========================================================
! ===================  Source Catalog  =====================
! ==========================================================

!srcfile = ./sources.spirals.dat

srccat/
source = 'G009.62-0.19'    RA= 18:06:14.67   DEC= -20:31:32.4  EQUINOX='J2000' VEL=  1.3 FLUX=6000.0 REMARKS= "G9" /
source = 'G009.62_off'     RA= 18:10:14.67   DEC= -21:31:32.4  EQUINOX='J2000' /
endcat /

! ==========================================================
! ===================  Setup Information  ==================
! ==========================================================

setinit = maser.meth /
  station = CEDUNA, WARK30M
  pcal = 'off'
  bits = 2
  nchan = 8
  bbfilter = 16
  freqref = 6658.5
  freqoff = 0,1,2,3,0,1,2,3
  netside = u,u,u,u,u,u,u,u
  pol = R,R,R,R,L,L,L,L
  bbc = 1,2,3,4,5,6,7,8 /
endset /

! ==========================================================
! ==================  Observation Schedule  ================
! ==========================================================

setup    = 'maser.meth'

stations = CEDUNA, WARK30M

LST      = CEDUNA
year     = 2021
month    = 11
day      = 23
start    = 15:00:00

! SCANS START ----------------------------------------------------

group=1 repeat=30
  source='1921-293'  gap=0:15  dur=0:45  /
gap=0

dwell = 5:00
group 2 repeat 5
  source = 'G009.62-0.19' /
  source = 'G009.62_off'  /

dwell = 60
source='1921-293' /

dwell = 5:00
group 2 repeat 5
  source = 'G009.62-0.19'  /
  source = 'G009.62_off'   /

dwell = 60
source='1921-293' /

! SCANS END ------------------------------------------------------
