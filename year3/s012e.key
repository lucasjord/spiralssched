! ==========================================================
! ==================  Observation Schedule  ================
! ==========================================================

setup    = 'spirals2.meth'

stations = CEDUNA, YARRA12M, KATH12M, WARK30M, HOBART26, HOBART12

LST      = CEDUNA
year     = 2023
month    = 8
day      = 9
start    = 13:30:00

! SCANS START ----------------------------------------------------

! Initial fringe finder (1 hour)
group=1 repeat=60
  source='3C279'  gap=0:30  dur=0:30 /
gap=0:15

stations = CEDUNA, KATH12M, WARK30M, HOBART26, HOBART12

dwell  = 2:00
source = 'J1850-0906' / 

group 3 repeat 15
    source='J1825-0737' dur=1:00 / !150mJy
    source='G023.88+0.06' dur=1:00 /
    source='J1850-0906' dur=2:00 / !70mJy?

stations = CEDUNA, YARRA12M, KATH12M, WARK30M, HOBART12

group 3 repeat 15
    source='J1825-0737' dur=1:00 / !150mJy
    source='G023.88+0.06' dur=1:00 /
    source='J1850-0906' dur=2:00 / !70mJy?

dwell = 1:00
source='1921-293' /

dwell  = 2:00
source = 'J1850-0906' / 

group 3 repeat 30
    source='J1825-0737' dur=1:00 /
    source='G023.88+0.06' dur=1:00 /
    source='J1850-0906' dur=2:00 /

dwell = 1:00
source='1921-293' /
stations = CEDUNA, YARRA12M, KATH12M, HOBART12

dwell  = 2:00
source = 'J1850-0906' / 

group 3 repeat 30
    source='J1825-0737' dur=1:00 /
    source='G023.88+0.06' dur=1:00 /
    source='J1850-0906' dur=2:00 /

dwell = 1:00
source='1921-293' /