  station = CEDUNA
  pcal = 'off'
  bits = 2
  nchan = 8
  bbfilter = 32
  freqref = 6580
  freqoff = 32,96,32,96,32,96,32,96
  netside = u,u,l,l,u,u,l,l
  pol = R,R,R,R,L,L,L,L
